{
  "name": "@mojaloop/interop-apis-bc-fspiop-api-svc",
  "version": "0.1.4",
  "description": "Mojaloop vNext FSPIOP API Service",
  "license": "Apache-2.0",
  "homepage": "https://github.com/mojaloop/interop-apis-bc/tree/main/modules/fspiop-api-svc",
  "private": true,
  "bugs": {
    "url": "https://github.com/mojaloop/interop-apis-bc/issues#readme"
  },
  "contributors": [
    "Pedro Sousa Barreto <pedrosousabarreto@gmail.com>",
    "Jose Antunes <jose.antunes@arg.software>",
    "Rui Rocha <rui.rocha@arg.software>"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "npm run service",
    "start:dev": "npm run build && AUDIT_KEY_FILE_PATH=./dist/tmp_key_file npm start",
    "service": "node dist/index.js",
    "build": "tsc",
    "watch": "tsc -w",
    "clean:dist": "rm -Rf dist",
    "clean:npm": "rm -Rf node_modules",
    "pretest": "npm run build",
    "standard": "npm run lint",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test:unit": "jest --passWithNoTests --testMatch **/test/unit/**/*.test.ts",
    "test:integration": "jest --passWithNoTests --rootDir='../../' --testMatch **/test/integration/**/*.test.ts --runInBand --detectOpenHandles --coverage --forceExit",
    "test:coverage": "jest --rootDir='../../' --passWithNoTests --coverage --coverageThreshold='{}' --testMatch **/test/integration/**/*.test.ts --runInBand --detectOpenHandles --forceExit",
    "test:coverage-check": "jest --rootDir='../../' --coverage --testMatch **/test/integration/**/*.test.ts --runInBand --detectOpenHandles --forceExit",
    "audit:resolve": "SHELL=sh resolve-audit",
    "audit:check": "SHELL=sh check-audit",
    "dep:check": "ncu -e 2",
    "dep:update": "ncu -e 2 -u"
  },
  "dependencies": {
    "@mojaloop/auditing-bc-client-lib": "~0.0.11",
    "@mojaloop/auditing-bc-public-types-lib": "~0.0.12",
    "@mojaloop/logging-bc-client-lib": "~0.1.18",
    "@mojaloop/logging-bc-public-types-lib": "^0.1.15",
    "@mojaloop/participant-bc-public-types-lib": "~0.0.7",
    "@mojaloop/participants-bc-client-lib": "~0.1.3",
    "@mojaloop/platform-shared-lib-public-messages-lib": "~0.1.70",
    "@mojaloop/platform-shared-lib-messaging-types-lib": "~0.2.70",
    "@mojaloop/platform-shared-lib-nodejs-kafka-client-lib": "~0.3.31",
    "@mojaloop/security-bc-client-lib": "^0.1.12",
    "ajv": "^8.12.0",
    "axios": "^1.3.3",
    "express": "~4.18.2"
  },
  "devDependencies": {
    "@types/express": "~4.17.17",
    "@types/supertest": "^2.0.12",
    "jest-openapi": "^0.14.2",
    "kafka-node": "^5.0.0",
    "wait-for-expect": "^3.0.2"
  }
}
